{
	"name": "bmack/modern-template-building",
	"description": "A homage to the old-style marker-based templating of TYPO3. Includes TEMPLATE and FILE cObjects.",
	"type": "typo3-cms-extension",
	"license": "GPL-2.0-or-later",
	"require": {
		"typo3/cms-core": "^12.0 || ^13.0",
		"typo3/cms-frontend": "^12.0 || ^13.0"
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "modern_template_building"
		}
	},
	"autoload": {
		"psr-4": {
			"Bmack\\ModernTemplateBuilding\\": "Classes/"
		}
	}
}
